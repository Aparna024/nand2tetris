/**
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=foo);
    And(a=a, b=foo, out=bar);
    And(a=b, b=sel, out=baz);
    Or(a=bar, b=baz, out=out);
}