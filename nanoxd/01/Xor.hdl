/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:

    Not(in = a, out = a1);
    And (a = a1, b = b, out = w1);

    Not(in = b, out = b1);
    And(a = b1, b = a, out = w2);

    Or(a = w1, b = w2, out = out);
}
